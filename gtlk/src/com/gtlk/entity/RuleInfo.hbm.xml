<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >	
	<class name="com.gtlk.entity.RuleInfo" table="t_ruleinfo">  
    	<id access="field" name="oid">
			<generator class="native" />
		</id> 
		<property name="creatTime" type="java.lang.String" column="creatTime" length="20" access="field"/>  
		<property name="rule" type="java.lang.String"  column="rule" length="2000" access="field" 
			not-null="true" />
    </class>  
</hibernate-mapping>
