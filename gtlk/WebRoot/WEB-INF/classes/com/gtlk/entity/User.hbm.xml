<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >	
	<class name="com.gtlk.entity.User" table="t_user">
		<id access="field" name="oid">
			<generator class="native" />
		</id>  
    	<property name="userName" type="java.lang.String" column="userName" length="10" access="field" not-null="true"/>   
		<property name="password" type="java.lang.String"  column="password" access="field" 
			not-null="true" />
		<property name="identity" type="java.lang.String"  column="identity" access="field" 
			not-null="true" />
    </class>  
</hibernate-mapping>
